    "@uniswap/lib": "^1.1.4",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "chai": "^4.2.0",
    "ganache-cli": "^6.11.0",
    "js-big-decimal": "^1.3.1",
    "openzeppelin-test-helpers": "^0.5.1",
    "remixd": "^0.2.3-alpha.7",
    "truffle": "^5.1.43",
    "truffle-privatekey-provider": "^1.5.0",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@truffle/hdwallet-provider": "^1.0.40",
    "concurrently": "^5.3.0"
  }
}
