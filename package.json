{
  "name": "contract-dexe-asset-management",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "truffle console --network",
    "bsc-test": "truffle test --network bsc_test",
    "build": "truffle compile",
    "test": "truffle test ./test/PancakePathFinder-test.js ./test/TestTraderContract-upgradeable.js --network test",
    "test-trader": "truffle test ./test/TestTraderContract-upgradeable.js --network test",
    "full-test": "concurrently --kill-others \"npm run ganache\" \"npm run test\"",
    "full-test-bsc": "concurrently --kill-others \"npm run ganachebsc\" \"npm run test\"",
    "compile": "truffle compile",
    "remixd": "remixd -s ./contracts/ --remix-ide https://remix.ethereum.org",
    "ganache": "ganache-cli --networkId 5777 -l 40000000 -q -f https://mainnet.infura.io/v3/31d99ba869424488ac90605233d4da0e -a ${ACCOUNT_AMOUNT:-10} -s 50a41a049751cc9c6b5317dfb94ff1751a1111f7780e09fc34102a0ae6b974a0ec3e382327d9f0cbd8c56535b78caf35636e56eabf6dd5661ed9b54325cd54dc",
    "ganachebsc": "ganache-cli -f https://bsc-dataseed1.binance.org -q -i 5777 --hardfork istanbul -l 70000000 -e 10000 -a ${ACCOUNT_AMOUNT:-10} -s 50a41a049751cc9c6b5317dfb94ff1751a1111f7780e09fc34102a0ae6b974a0ec3e382327d9f0cbd8c56535b78caf35636e56eabf6dd5661ed9b54325cd54dc",
    "migrate": "truffle migrate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openzeppelin/contracts": "^3.1.0",
    "@chainlink/contracts": "^0.0.8",
